#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/github2/leo-interview/node_modules/.pnpm/vitest@3.2.4_jiti@2.6.1_jsd_429de3d1fe7bf31fa9b055f95a5e5bc3/node_modules/vitest/node_modules:/mnt/d/github2/leo-interview/node_modules/.pnpm/vitest@3.2.4_jiti@2.6.1_jsd_429de3d1fe7bf31fa9b055f95a5e5bc3/node_modules:/mnt/d/github2/leo-interview/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/github2/leo-interview/node_modules/.pnpm/vitest@3.2.4_jiti@2.6.1_jsd_429de3d1fe7bf31fa9b055f95a5e5bc3/node_modules/vitest/node_modules:/mnt/d/github2/leo-interview/node_modules/.pnpm/vitest@3.2.4_jiti@2.6.1_jsd_429de3d1fe7bf31fa9b055f95a5e5bc3/node_modules:/mnt/d/github2/leo-interview/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vitest@3.2.4_jiti@2.6.1_jsd_429de3d1fe7bf31fa9b055f95a5e5bc3/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vitest@3.2.4_jiti@2.6.1_jsd_429de3d1fe7bf31fa9b055f95a5e5bc3/node_modules/vitest/vitest.mjs" "$@"
fi
